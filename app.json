{
  "expo": {
    "name": "AppMyMovies",
    "slug": "appmymovies",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/samufilmessplash5.png",
      "resizeMode": "cover",
      "backgroundColor": "#141A29"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/samufilmesicon.png",
        "backgroundColor": "#141A29"
      },
      "package": "com.appmovies"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app"
    ],
    "extra": {
      "eas": {
        "projectId": "1e8c0bad-013b-4c0d-b5bf-58beefd63ecc"
      }
    },
    "owner": "samukavlr"
  }
}
